<Window x:Class="Diosk.PaymentWin"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Diosk"
        mc:Ignorable="d"
        Width="500" Height="300">
    <Grid>
        <Grid Background="White" Width="500" Height="275">
            <Grid.RowDefinitions>
                <RowDefinition Height="300"></RowDefinition>
            </Grid.RowDefinitions>
            <Grid Grid.Row="0">
                <Button x:Name="Cash" Height="50" Width="100" Click="Cash_Click" HorizontalAlignment="Left" Content="현금" Margin="100, 0, 100, 0"></Button>
                <Button x:Name="Card" Height="50" Width="100" Click="Card_Click" HorizontalAlignment="Right" Content="카드" Margin="100, 0, 100, 0"></Button>
            </Grid>
        </Grid>
    </Grid>
</Window>
